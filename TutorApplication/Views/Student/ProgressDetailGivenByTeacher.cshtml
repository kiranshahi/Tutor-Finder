@model IEnumerable<TutorApplication.Models.ViewModel.ProgressViewModel>

@{
    ViewBag.Title = "ProgressDetailGivenByTeacher";
    Layout = "~/Views/Shared/_LayoutStudent.cshtml";
}

<div class="row">

    <div class="span9">

        <div class="well">
            <h3>Select Student To Add Progress</h3>
            <hr class="soft" />
<table class="table table-bordered table-hover">
    <tr>
        
        <th>
            @Html.DisplayNameFor(model => model.Comment)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PresentDate)
        </th>
       
    </tr>

@foreach (var item in Model) {
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.Comment)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PresentDate)
        </td>
        
    </tr>
}

</table>
            @Html.ActionLink("Back To List", "GetTeacherAssignProgress")
            </div>
        </div>
    </div>
