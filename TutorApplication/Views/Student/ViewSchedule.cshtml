@model IEnumerable<TutorApplication.Models.ViewModel.ScheduleViewModel>

@{
    ViewBag.Title = "ViewSchedule";
    Layout = "~/Views/Shared/_LayoutStudent.cshtml";
}

<div class="row">

    <div class="span9">

        <div class="well">
            <h3>List Of Time</h3>
            <hr class="soft" />
            <table class="table table-hover">
                <tr>

                    <th>
                        @Html.DisplayNameFor(model => model.FromTime)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.ToTime)
                    </th>

                    <th>
                        @Html.DisplayNameFor(model => model.Status)
                    </th>
                    <th></th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>

                        <td>
                            @Html.DisplayFor(modelItem => item.FromTime)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ToTime)
                        </td>

                        <td>
                            @Html.DisplayFor(modelItem => item.Status)
                        </td>
                        <td>
                            @Html.ActionLink("Book Now", "BookNow", new { id = item.ScheduleId }) |
          
                        </td>
                    </tr>
                }

            </table>
             @Html.ActionLink("Back To Teacher List", "ViewTutor","Student")
        </div>
    </div>
</div>
